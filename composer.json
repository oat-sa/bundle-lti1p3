{
    "name": "oat-sa/bundle-lti1p3",
    "description": "OAT LTI 1.3 Symfony Bundle",
    "keywords": ["symfony", "LTI1p3"],
    "type": "symfony-bundle",
    "license": "LGPL-2.1",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "oat-sa/lib-lti1p3-core": "dev-feature/service-handlers",
        "symfony/framework-bundle": "^4.4 || ^5.1",
        "symfony/security-bundle": "^4.4 || ^5.1",
        "symfony/security-http": "^4.4.1 || ^5.1",
        "symfony/yaml": "^4.4 || ^5.1",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.14",
        "php-coveralls/php-coveralls": "^2.4",
        "symfony/browser-kit": "^4.4 || ^5.1"
    },
    "autoload": {
        "psr-4": {
            "OAT\\Bundle\\Lti1p3Bundle\\": "",
            "OAT\\Library\\Lti1p3Core\\Tests\\": "vendor/oat-sa/lib-lti1p3-core/tests/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
