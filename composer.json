{
    "name": "oat-sa/bundle-lti1p3",
    "description": "OAT LTI 1.3 Symfony Bundle",
    "keywords": ["symfony", "LTI1p3"],
    "type": "symfony-bundle",
    "license": "LGPL-2.1",
    "require": {
        "php": ">=8.1.0",
        "ext-json": "*",
        "league/oauth2-server": "^8.5",
        "nyholm/psr7": "^1.8",
        "oat-sa/lib-lti1p3-core": "^7.0.2",
        "psr/log": "^1 || ^2 || ^3",
        "symfony/framework-bundle": "^6.4",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/security-bundle": "^6.4",
        "symfony/security-core": "^6.4",
        "symfony/security-http": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "require-dev": {
        "colinodell/psr-testlogger": "^1.2",
        "guzzlehttp/guzzle": "^7.8",
        "nesbot/carbon": "^2.72 || ^3.0",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "~9",
        "psalm/plugin-phpunit": "^0.18",
        "symfony/browser-kit": "^6.4",
        "vimeo/psalm": "^5.25"
    },
    "autoload": {
        "psr-4": {
            "OAT\\Bundle\\Lti1p3Bundle\\": "",
            "OAT\\Library\\Lti1p3Core\\Tests\\": "vendor/oat-sa/lib-lti1p3-core/tests/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
